own memmove func:

#include<stdio.h>
#include<string.h>
char memovefun(void *dest,void *src,int l)
{
    char new[20];
    char *s=(char*)src;
    char *d=(char*)dest;
    char *temp=new;
    for(int i=0;i<l;i++)
    {
        temp[i]=s[i];
    }
    for(int i=0;i<l;i++)
    {
        d[i]=temp[i];
    }
    printf("%s",d);
}
int main()
{
char s[]="kirankumar";
char d[20];
int l=(strlen(s)+1);
memovefun(d,s,l);
}


another way:
#include<stdio.h>
#include<string.h>
char memovefun(void *dest,void *src,int l)
{
    
    char *s=(char*)src;
    char *d=(char*)dest;
    char temp[20];
    for(int i=0;i<l;i++)
    {
        temp[i]=s[i];
    }
    for(int i=0;i<l;i++)
    {
        d[i]=temp[i];
    }
    printf("%s",d);
}
int main()
{
char s[]="kirankumar";
char d[20];
int l=(strlen(s)+1);
memovefun(d,s,l);
}

using libr function:

#include<stdio.h>
#include<string.h>
int main()
{
    char s[]="kirankumar";
    char d[20];
    int l=(strlen(s)+1);
    memmove(d,s,l);
    printf("using libr func:%s",d);
}


