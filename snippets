#include <stdio.h>
#define x 5+2
int main()
{
    int i;
    i=x*x*x; // here we using the bodmas rule 1st it takes the multiplications then addition: 5+2*5+2*5+2=5+10+10+2=27;
    printf("%d",i);
    return 0;
}
output:
27


#include <stdio.h>
#include<string.h>
int main()
{
   char s[]={'a','b','k','i','r','k','a','n'};
   int l=strlen(s);
  char *k=memchr(s,'k',l);
  printf("%c",k[0]);
  printf("%c",k[1]);
  printf("%c",k[2]);
}


